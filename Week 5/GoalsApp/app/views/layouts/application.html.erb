<!DOCTYPE html>
<html>
<head>
  <title>GoalApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= flash.now[:errors] %>

<% if current_user %>
<%= button_to "Sign Out", session_url, :method => :delete %>
<%= current_user.username %>
<% end %>

<%= yield %>

</body>
</html>
